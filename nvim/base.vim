let mapleader =","
set wildmode=longest,list,full
set nocompatible
filetype off
filetype plugin on
set nocp
set nofoldenable
syntax on
filetype plugin indent on
set tabstop=4
set shiftwidth=4
set expandtab
set nobackup
set noswapfile
set nohlsearch
set go=a
set splitbelow splitright
set number relativenumber
let g:python3_host_prog='/usr/bin/python3.8'
let g:python_host_prog='/usr/bin/python'
